{
  "name": "shaka-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run front\" \"npm run server\" ",
    "front": "ng serve --host 0.0.0.0 --port 4200",
    "server": " cd ../server && npm start",
    "start-awsdev-env": "concurrently \"npm run front-awsdev-env\" \"npm run server-awsdev-env\" ",
    "front-awsdev-env": "ng serve --host 0.0.0.0 --port 5000",
    "server-awsdev-env": " cd ../server && npm start",
    "start-docker-env": "concurrently \"npm run front_docker\" \"npm run server\" ",
    "front_docker": "ng serve --host 0.0.0.0 --disableHostCheck --port 5000",
    "build": "ng build",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@neomorphism/ng-neomorphism": "^0.46.1",
    "@ngrx/effects": "^8.6.0",
    "@ngrx/router-store": "^8.6.0",
    "@ngrx/store": "^8.6.0",
    "@ngrx/store-devtools": "^8.6.0",
    "angular-plotly.js": "^1.5.0",
    "aws-cloudfront-sign": "^2.2.0",
    "concurrently": "^5.0.1",
    "formidable": "^1.2.2",
    "hammerjs": "^2.0.8",
    "normalize.css": "^8.0.1",
    "plotly.js": "^1.52.0",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.5.4",
    "shaka-player": "^2.5.6",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.10",
